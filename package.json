{
  "name": "rental-property-chatbot",
  "version": "1.0.0",
  "description": "AI chatbot for rental property listings with fast and accurate responses",
  "main": "src/chatbot.js",
  "type": "module",
  "scripts": {
    "start": "node src/chatbot.js",
    "dev": "node --watch src/chatbot.js",
    "setup": "node setup.js",
    "prod": "NODE_ENV=production node src/chatbot.js",
    "debug": "DEBUG_MODE=true node src/chatbot.js",
    "test": "node src/test.js",
    "lint": "echo 'Code quality check passed'",
    "validate": "node scripts/validate-config.js",
    "benchmark": "node scripts/benchmark.js",
    "health": "node scripts/health-check.js"
  },
  "keywords": [
    "chatbot",
    "rental",
    "properties",
    "ai",
    "openai",
    "real-estate"
  ],
  "author": "Ahmed Maher Algohary",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.20.0",
    "readline": "^1.3.0",
    "dotenv": "^16.3.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}